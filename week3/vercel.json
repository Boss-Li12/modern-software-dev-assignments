{
    "version": 2,
    "builds": [
        {
            "src": "server/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/main.py"
        }
    ],
    "env": {
        "MCP_API_KEY": "@mcp_api_key"
    }
}